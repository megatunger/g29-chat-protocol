{
  "name": "g29-chat-protocol",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "pretty": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json}\"",
    "generate:keypair": "node scripts/generate-keypair.js",
    "generate:key": "node scripts/generate-keypair.js key1",
    "test": "node --test test/**/*.test.js",
    "start": "fastify start -l info app.js",
    "create:db": "prisma migrate reset",
    "dev:without-key": "fastify start -w -l info -P app.js",
    "dev": "SERVER_ID=G29_SERVER1 SERVER_KEY_ID=key1 yarn dev:without-key",
    "dev:server2": "SERVER_ID=G29_SERVER2 SERVER_KEY_ID=key1 yarn dev:without-key --port 3002",
    "studio": "npx prisma studio"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^6.0.0",
    "@fastify/cors": "^11.1.0",
    "@fastify/sensible": "^6.0.0",
    "@fastify/websocket": "^11.2.0",
    "@prisma/client": "^6.16.2",
    "base64url-universal": "^2.0.0",
    "bcrypt": "^6.0.0",
    "date-fns": "^4.1.0",
    "fastify": "^5.0.0",
    "fastify-cli": "^7.4.0",
    "fastify-plugin": "^5.0.0",
    "jsonwebtoken": "^9.0.2",
    "openpgp": "^6.2.2",
    "sqlite3": "^5.1.7",
    "ws": "^8.18.3",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "prisma": "^6.16.2"
  }
}
